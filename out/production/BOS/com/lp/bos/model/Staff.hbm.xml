<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lp.bos.model.Staff" table="bc_staff">
        <id name="id" column="id">
            <generator class="assigned"/>
        </id>
        <property name="name" column="name"/>
        <property name="telephone" column="telephone"/>
        <property name="haspda" column="haspda"/>
        <property name="deltag" column="deltag"/>
        <property name="station" column="station"/>
        <property name="standard" column="standard"/>
    </class>
    <query name="staff_delete">update Staff set deltag = ? where id = ?</query>
</hibernate-mapping>